var prinr = prompt("Digite o primeiro número");

var segnr = prompt("Digite o segundo número");

var resultado = Number(prinr) + Number(segnr);

alert("O resultado da soma é: " + resultado);

if (resultado > 100) {
    alert("Esta calculadora é limitada, você atingiu o valor máximo de cálculo.");
} else {
    if (resultado > 50) {
        alert("Voce pode somar valores até 100")
    } else {
        if (resultado > 5) {
            alert("Some valore mais altos")
        } else {
            alert.log("Valor muito baixo heinnn....")
        }
    }
}


